{
	"name": "@oniya-with-chats/nestjs-twurple-eventsub-ws",
	"version": "0.1.7",
	"repository": {
		"type": "git",
		"url": "https://github.com/oniya-with-chats/nestjs-twurple.git",
		"directory": "packages/api"
	},
	"publishConfig": {
		"access": "restricted",
		"registry": "https://npm.pkg.github.com/"
	},
	"private": "true",
	"description": "A NestJS wrapper for @twurple/eventsub-ws package",
	"keywords": [
		"nestjs",
		"nest",
		"twurple",
		"twitch",
		"eventsub",
		"websocket",
		"stream",
		"bot",
		"events"
	],
	"sideEffects": false,
	"main": "lib",
	"types": "lib",
	"engines": {
		"node": ">=16"
	},
	"homepage": "https://github.com/stimulcross/nestjs-twurple",
	"author": "Stimul Cross <stimulcross@gmail.com>",
	"license": "MIT",
	"scripts": {
		"build": "tsc -p ./tsconfig.json",
		"rebuild": "yarn clear && yarn build",
		"clear": "rimraf lib",
		"test": "jest --config \"../../jest.config.js\" --selectProjects eventsub-ws --runInBand --detectOpenHandles --forceExit",
		"test:verbose": "jest --config \"../../jest.config.js\" --selectProjects eventsub-ws --runInBand --verbose --detectOpenHandles --forceExit"
	},
	"peerDependencies": {
		"@nestjs/common": "*",
		"@nestjs/core": "*",
		"@twurple/api": "^7",
		"@twurple/eventsub-ws": "^7",
		"reflect-metadata": "^0.2.2",
		"rxjs": "*"
	},
	"devDependencies": {
		"@nestjs/common": "^10.2.7",
		"@nestjs/core": "^10.2.7",
		"@twurple/api": "^7.0.3",
		"@twurple/eventsub-ws": "^7.0.3",
		"reflect-metadata": "^0.2.2",
		"rimraf": "4.4.1",
		"rxjs": "^7.8.1"
	},
	"files": [
		"LICENSE",
		"README.md",
		"lib",
		"!lib/**/*.d.ts.map"
	]
}
